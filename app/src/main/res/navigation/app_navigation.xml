<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/app_navigation"
            app:startDestination="@id/startFragment">

    <fragment android:id="@+id/editorFragment" android:name="com.example.sergiobelda.photoeditor.uifragments.EditorFragment"
              android:label="fragment_editor" tools:layout="@layout/fragment_editor">
        <argument android:name="image" app:argType="string"/>
    </fragment>
    <fragment android:id="@+id/startFragment" android:name="com.example.sergiobelda.photoeditor.uifragments.StartFragment"
              android:label="StartFragment">
        <action android:id="@+id/editorAction" app:destination="@id/editorFragment"/>
    </fragment>
</navigation>